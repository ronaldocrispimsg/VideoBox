#no front poe... para assistir os videos do stream repo
var player = new Hls();
player.loadSource("http://IP_STREAM/streams/<video_id>/playlist.m3u8");
player.attachMedia(videoElement);


#outro exemplo
import Hls from "hls.js";

const video = document.getElementById("player");

if (Hls.isSupported()) {
    const hls = new Hls();
    hls.loadSource("http://18.230.10.55/streams/abc123/playlist.m3u8");
    hls.attachMedia(video);
}

